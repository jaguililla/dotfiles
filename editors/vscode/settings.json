{
    "workbench.colorTheme": "Dare Contrast (rainglow)",
    "editor.cursorStyle": "underline",
    "files.autoSave": "onFocusChange",
    "breadcrumbs.enabled": false,
    "window.menuBarVisibility": "toggle",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        100
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 100,
    "editor.wordWrapColumn": 100,
    "editor.renderLineHighlight": "all",
    "workbench.editor.tabCloseButton": "off",
    "extensions.autoUpdate": true,
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.textwidth": 100,
    "editor.minimap.showSlider": "always",
    "editor.formatOnType": false,
    "editor.formatOnPaste": true,
    "editor.autoIndent": "full",
    "editor.folding": false,
    "editor.dragAndDrop": false,
    "editor.formatOnSave": true,
    "workbench.startupEditor": "none",
    "window.title": "${folderName} | ${activeEditorMedium} ${dirty}",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "editor.fontSize": 14,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "window.zoomLevel": 0,
    "git.autofetch": true,
    "editor.mouseWheelZoom": true,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorWidth": 2,
    "editor.find.autoFindInSelection": "always",
    "files.trimFinalNewlines": true,
    "workbench.fontAliasing": "antialiased",
    "workbench.settings.useSplitJSON": true,
    "window.titleBarStyle": "native",
    "zenMode.centerLayout": false,
    "terminal.integrated.fontSize": 14,
    "http.proxySupport": "off",
    "keyboard.dispatch": "keyCode",
    "rest-client.enableSendRequestCodeLens": false,
    "rest-client.enableCustomVariableReferencesCodeLens": false,
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.iconTheme": "simple-icons",
    "workbench.tree.indent": 22,
    "redhat.telemetry.enabled": false,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "vsicons.dontShowNewVersionMessage": true,
    "livePreview.notifyOnOpenLooseFile": false,
    "godot_tools.editor_path": "/usr/bin/godot"
}
